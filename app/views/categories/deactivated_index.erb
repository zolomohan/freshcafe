<h1 class="mt-4 ">Deactivated Categories</h1>
<hr class="mb-5">

<% if @categories.count == 0 %>
    <p class="lead text-muted">No Deactivated Categories</p>
<% else %>
    <%= render 'list', items:@categories %>
    <div class="my-5 pb-5">
        <%= will_paginate renderer: WillPaginateSemanticUi::ActionView::Renderer %>
    </div>
<% end %>